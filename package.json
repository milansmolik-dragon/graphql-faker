{
  "name": "graphql-faker-sampled",
  "version": "3.1.1",
  "description": "Mock or extend your GraphQL API with faked data. No coding required",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon -e ts --exec 'ts-node src/index.ts'",
    "debug": "ts-node --inspect --compilerOptions '{\"inlineSources\":true}' src/index.ts",
    "build:typescript": "tsc",
    "copy:graphql": "cp src/*.graphql dist/",
    "build:all": "npm run build:typescript && npm run copy:graphql"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milansmolik-dragon/graphql-faker-sampled"
  },
  "author": "Milan Smolik <milan.smolik@projectdrgn.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/cors": "2.8.5",
    "@types/express": "4.17.0",
    "@types/express-graphql": "^0.8.0",
    "@types/faker": "4.1.5",
    "@types/graphql": "14.2.2",
    "@types/lodash": "4.14.136",
    "@types/yargs": "13.0.0",
    "ts-node": "8.3.0",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-graphql": "0.9.0",
    "faker": "4.1.0",
    "graphql": "^14.1.1",
    "graphql-tools": "^4.0.5",
    "lodash": "4.17.14",
    "merge-graphql-schemas": "^1.5.8",
    "moment": "2.24.0",
    "node-fetch": "2.6.0"
  }
}
